{
  "name": "bags_brats_workspace",
  "version": "1.0.0",
  "description": "Bags & Brats Tournament Management System",
  "scripts": {
    "start": "docker compose up",
    "stop": "docker compose down",
    "build": "docker compose build",
    "test": "cd tests && npx playwright test",
    "test:ui": "cd tests && npx playwright test --ui",
    "seed": "docker compose exec backend python seed_players.py"
  },
  "devDependencies": {
    "playwright": "latest"
  },
  "dependencies": {
    "@lancedb/lancedb": "^0.23.0",
    "@xenova/transformers": "^2.17.2"
  },
  "optionalDependencies": {
    "@lancedb/lancedb-darwin-x64": "^0.23.0"
  }
}
